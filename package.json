{
  "name": "tresor-de-famille",
  "version": "1.0.1",
  "description": "trÃ©sor de famille application",
  "scripts": {
    "start": "node dist/server.js",
    "build": "yarn build:front && tsc --p tsconfig.build.json && yarn postcss:build && cp -r src/assets dist",
    "build:front": "webpack --config=webpack.config.js",
    "postcss:local": "postcss -o src/assets/style.css src/pages/style.css",
    "postcss:build": "postcss -o dist/assets/style.css src/pages/style.css",
    "watch": "concurrently \"nodemon src/server.ts\" \"yarn watch:front\" ",
    "watch:debug": "yarn postcss:local && concurrently \"nodemon --inspect src/server.ts\" \"yarn watch:front\" ",
    "watch:front": "webpack --config=webpack.config.js --watch",
    "prettier": "prettier --write . --ignore-unknown",
    "test": "jest --runInBand",
    "test:watch": "jest --runInBand --watch",
    "lint": "eslint -c ./eslintrc.js src",
    "db": "docker compose down --remove-orphans && docker compose up -d",
    "db:test": "export NODE_ENV=test && export COMPOSE_PROJECT_NAME=tdf_tests && docker compose down --remove-orphans && docker compose -f ./docker-compose-integration.yml up -d && until docker exec tdf_db_tests pg_isready -U test -d tdf_test; do sleep 1; done",
    "storybook": "storybook dev -p 6006",
    "sb": "storybook dev -p 6006",
    "build-storybook": "build-storybook",
    "psql": "psql -h localhost -p 5434 -d tdf -U admin -W",
    "psql:test": "psql -h localhost -p 5435 -d tdf_test -U test -W",
    "psql:prod": "scalingo -a tresor-de-famille pgsql-console",
    "devtools": "nodemon --config nodemon.devtools.json --inspect src/facts/diagram/diagramServer.ts"
  },
